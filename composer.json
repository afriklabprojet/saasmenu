{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.2",
        "doctrine/dbal": "^3.10",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^3.8",
        "josiasmontag/laravel-recaptchav3": "^1.0",
        "ladumor/laravel-pwa": "^0.0.5",
        "laravel/framework": "^10.49",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.15",
        "laravel/tinker": "^2.9",
        "league/omnipay": "^3.2",
        "maatwebsite/excel": "^3.1",
        "mollie/laravel-mollie": "^2.25",
        "myfatoorah/laravel-package": "^2.1",
        "omnipay/paypal": "^3.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-analytics": "^4.1",
        "spatie/laravel-cookie-consent": "^3.3",
        "stripe/stripe-php": "^10.21",
        "tecnickcom/tcpdf": "^6.10",
        "xendit/xendit-php": "^2.19"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.17",
        "laravel/sail": "^1.31",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.12",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.8"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
